    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
</head>
<body>
    <div class="wrapper">
        <div class="header">
            <div class="title">
                <h1>Harper's Choice - Music in our School Month</h1>
            </div>
        </div>
        <div class="body">
            <p>Welcome to Blah blah blah</p>
            <form method="POST" id="config_form" action="${request.current_route_url()}" accept-charset="utf-8" enctype="multipart/form-data">
                <label for="photo">Photo</label>
                <input type="file" name="photo" id="photo">
                <br>
                <textarea name="caption" id="caption_box"></textarea>
                <br>
                <input type="submit" name="photo.save" value="Submit">
            </form>
        </div>
        <div class="footer">
            <p>Made by D.J. Figueroa</p>
        </div>
    </div>
</body>
