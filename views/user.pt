    <div class="wrapper">
        <div class="header">
            <div class="title">
                <a href="/"><img src="https://hcms.figsware.net/static/image/HCMSPhoto.png"></a>
                <h1>Harper's Choice - Music in our School Month</h1>
            </div>
            <div class="nav">
                <ul>
                    <li><a href="/upload">Upload</a></li>
                    <li><a href="${user_link}">${user_text}</a></li>
                </ul>
            </div>
        </div>
        <div class="body">
            <h1>The user has ${points} points.</h1>
            <button onclick="givePoint(1)">Give Point</button>
            <button onclick="takePoint(1)">Take Point</button>
            <div id="userPhotos">
